{
  "name": "@iv-ingestion/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for IV Ingestion API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc src/index.ts --out docs",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "iv-ingestion",
    "api",
    "sdk",
    "typescript",
    "javascript",
    "home-inspection",
    "file-processing"
  ],
  "author": "IV Ingestion Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iv-ingestion/sdk-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/iv-ingestion/sdk-javascript/issues"
  },
  "homepage": "https://docs.iv-ingestion.com/sdk/javascript",
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "typedoc": "^0.25.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
} 